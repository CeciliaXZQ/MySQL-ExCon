<!-- Takes in the credentials of the outlook account-->
<html>
<head>
    <title>Outlook Synchronization</title>
    <meta name="decorator" content="atl.admin">
</head>
<body>
<form id="admin" class="aui" action="" method="POST">
    <div class="field-group">
        <label for="name">Name:</label>
        <input type="text" id="name" name="name" class="text">
    </div>
    <div class="field-group">
        <label for="password">Password:</label>
        <input type="password" id="password" name="password" class="text">
    </div>
    <div class="field-group">
        <label for="ParentID">Calendar Name:</label>
        <input type="text" id="ParentID" name="ParentID" class="text">
    </div>
    <div class="field-group">
        <input type="submit" value="Save" class="button">
    </div>
</form>
</body>
</html>